<script type="text/javascript">

$(document).ready(function(){

    $("#date1").datepicker();

    $("#date1").submit(function (event){
            //입력양식의 value를 가져옵니다
            var name=$("#date1").val();
		 alert(name);
	})

});

</script>

	<input type="text" name="date" id="date1" size="12" />
	<input type="submit" onclick="$('#date1').datepicker('show');" />

